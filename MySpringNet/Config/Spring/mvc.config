<?xml version="1.0" encoding="utf-8"?>

<objects xmlns="http://www.springframework.net" xmlns:aop="http://www.springframework.net/aop">
  
  <object name="RequireLoginAdvisor" type="Spring.Aop.Support.RegularExpressionMethodPointcutAdvisor, Spring.Aop">
    <property name="Patterns">
      <list>
        <value>MySpringNet.Controllers.HomeController.Index</value>
      </list>
    </property>
    <property name="Advice">
      <object type="MySpringNet.Controllers.AOP.RequireLoginAdvice,MySpringNet"></object>
    </property>
  </object>
 
  <object type="Spring.Aop.Framework.AutoProxy.InheritanceBasedAopConfigurer, Spring.Aop">
    <property name="ObjectNames">
      <list>
        <value>HomeController</value>
      </list>
    </property>
    <property name="InterceptorNames">
      <list>
        <value>RequireLoginAdvisor</value>
      </list>
    </property>
  </object>
    
   <object name="BaseController" abstract="true">
    <property name="PageSize" value="${PageSize}">
    </property>
  </object>

  <object name="HomeController" type="MySpringNet.Controllers.HomeController,MySpringNet" parent="BaseController" singleton="false">
    
  </object>
</objects>
